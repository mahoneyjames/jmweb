{
  "name": "badger",
  "version": "1.0.0",
  "description": "static site generator in node",
  "private": true,
  "engines": {
    "node": ">= 7.6.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "nodemon ./previewHost/preview.js",
    "run": "concurrently \"npm run assets\" \"npm run build\"",
    "build": "nodemon ./build.js --ignore _generated/ -e js,md,pug",
    "getmetadata": "node ./download.js",
    "assets": "webpack -w --display-max-modules 0"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "0.15.3",
    "dotenv": "4.0.0",
    "express": "4.15.2",
    "multer": "1.3.0",
    "pug": "2.0.0-beta6",
    "csv": "1.1.1",
    "csvtojson": "1.1.6",
    "moment": "2.18.1",
    "es6-promisify": "5.0.0",
    "showdown": "1.7.1",
    "mkdirp": "0.5.1",
    "jfile": "1.1.12",
    "front-matter": "2.1.2"
  },
  "devDependencies": {
    "autoprefixer": "6.7.7",
    "babel-core": "6.24.0",
    "babel-loader": "6.4.0",
    "babel-preset-es2015": "6.24.0",
    "browser-sync": "^2.18.13",
    "concurrently": "3.4.0",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "nodemon": "1.11.0",
    "css-loader": "0.27.3",
    "extract-text-webpack-plugin": "2.1.0",
    "node-sass": "4.5.3",    
